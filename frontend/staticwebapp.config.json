{
  "navigationFallback": {
    "rewrite": "/flower.html",
    "exclude": ["/images/*.{png,jpg,gif,webp}", "/css/*", "/*.js", "/*.css", "/*.json"]
  },
  "routes": [
    {
      "route": "/",
      "serve": "/flower.html"
    },
    {
      "route": "/product/*",
      "rewrite": "/detail.html"
    }
  ],
  "mimeTypes": {
    ".html": "text/html",
    ".css": "text/css",
    ".js": "text/javascript",
    ".json": "application/json"
  }
}
